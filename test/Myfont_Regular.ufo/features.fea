languagesystem DFLT dflt;
languagesystem latn dflt;


# GPOS 


lookup kernHorizontalKerninglookup0 {
 lookupflag 0;
    pos \A \A 40;
} kernHorizontalKerninglookup0;

lookup markMarkPositioninginLatinlookup2 {
  lookupflag 0;
  markClass [\acutecomb ] <anchor 0 589> @top;
  pos base [\A ] <anchor 274 755> mark @top;
} markMarkPositioninginLatinlookup2;

feature kern {

 script DFLT;
     language dflt ;
      lookup kernHorizontalKerninglookup0;
} kern;

feature mark {

 script DFLT;
     language dflt ;
      lookup markMarkPositioninginLatinlookup2;

 script latn;
     language dflt ;
      lookup markMarkPositioninginLatinlookup2;
} mark;
#Mark attachment classes (defined in GDEF, used in lookupflags)

@GDEF_Simple = [\A ];
@GDEF_Mark = [\acutecomb ];

table GDEF {
  GlyphClassDef @GDEF_Simple, , @GDEF_Mark, ;

} GDEF;

