languagesystem DFLT dflt;
languagesystem latn dflt;


# GPOS 


lookup kernHorizontalKerninglookup0 {
  lookupflag 0;
    pos \A \A 44;
} kernHorizontalKerninglookup0;

lookup markMarkPositioninginLatinlookup1 {
  lookupflag 0;
  markClass [\acutecomb ] <anchor 0 589> @top;
  pos base [\A ] <anchor 298 755> mark @top;
} markMarkPositioninginLatinlookup1;

feature kern {

 script DFLT;
     language dflt ;
      lookup kernHorizontalKerninglookup0;
} kern;

feature mark {

 script DFLT;
     language dflt ;
      lookup markMarkPositioninginLatinlookup1;

 script latn;
     language dflt ;
      lookup markMarkPositioninginLatinlookup1;
} mark;
#Mark attachment classes (defined in GDEF, used in lookupflags)

@GDEF_Simple = [\A ];
@GDEF_Mark = [\acutecomb ];

table GDEF {
  GlyphClassDef @GDEF_Simple, , @GDEF_Mark, ;

} GDEF;

